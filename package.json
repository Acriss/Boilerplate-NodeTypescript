{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "As I had to create a few API, I always lacked the boilerplate I could be happy with, so Im now making my own",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "startdev": "NODE_ENV=development npm run serve",
    "start": "NODE_ENV=production npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "tslint": "tslint =c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Acriss/Boilerplate-NodeTypescript"
  },
  "keywords": [
    "boilerplate",
    "node",
    "express",
    "typescript"
  ],
  "author": "Pierre MÃ©vel",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/express-validator": "^3.0.0",
    "@types/lusca": "^1.5.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "nodemon": "^1.18.5",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --verbose"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "lusca": "^1.6.1"
  }
}
