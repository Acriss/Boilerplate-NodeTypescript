{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "no-console": [true, "log", "warning"],
    "prefer-const": false,
    "trailing-comma": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}
